{
  "name": "js-dev-env-fe",
  "version": "1.0.0",
  "description": "Start point boilerplate for a front end development",
  "keywords": [],
  "author": "TomÃ¡s Ruiz",
  "license": "MIT",
  "scripts": {
    "add-to-bash": "npm completion >> ~/.bashrc && source ~/.bashrc",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "nyc --reporter=lcov --reporter=text mocha-webpack --reporter spec --webpack-config webpack.config.test.js  \"src/**/*.test.js\" -r buildScripts/testSetup.js -r jsdom-global/register --interactive false",
    "test:watch": "mocha-webpack --watch --reporter spec --webpack-config webpack.config.test.js  \"src/**/*.test.js\" -r buildScripts/testSetup.js -r jsdom-global/register --interactive false",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch buildScripts/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "build:only": "npm-run-all clean-dist test lint && babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "dependencies": {
    "@tomasruizr/trutils": "^2.0.1",
    "socket.io-client": "^2.2.0",
    "vue": "^2.5.16",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.19",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chalk": "2.4.1",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.2",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-watch": "3.1.5",
    "express": "4.16.3",
    "fetch-mock": "^6.4.4",
    "html-webpack-plugin": "3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "11.11.0",
    "jsdom-global": "^3.0.2",
    "json-schema-faker": "0.5.0-rc15",
    "json-server": "0.14.0",
    "localtunnel": "^1.9.1",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "4.1.3",
    "nyc": "^12.0.2",
    "opn": "^5.4.0",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.3",
    "sinon": "^6.0.0",
    "socket.io": "^2.2.0",
    "style-loader": "0.21.0",
    "surge": "^0.20.1",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "4.12.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-md5-hash": "0.0.6",
    "webpack-node-externals": "^1.7.2"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false,
    "all": true
  }
}
